{
	"headerHeight": "0px",
	"headerStyle": {
		"font-family": "Futura-Book",
		"padding": "5px",
		"display": "grid",
		"grid-template": "1fr / 1fr",
		"justify-items": "end",
		"align-items": "center",
		"background-color": "beige"
	},
	"filters": [
		{
			"name": "year",
			"label": "Raised Year",
			"params": {
				"source": "devmain",
				"value": "RAISEDYEAR",
				"display": "RAISEDYEAR",
				"sort": "-RAISEDYEAR"
			},
			"data": []
		},
		{
			"name": "status",
			"label": "Status",
			"params": {
				"source": "devmain",
				"value": "STATUS",
				"display": "STATUS",
				"sort": "STATUS"
			},
			"data": []
		}
	],
	"tabs": [
		{
			"label": "Performance",
			"nopad": true,
			"headers": [
				{
					"name": "hdr_charts",
					"items": [{"type":"filter","name":"year"}]
				},
				{
					"name": "hdr_list",
					"items": [{"type":"filter","name":"status"}]
				}
			],
			"tables": [
				{
					"name": "dev_list",
					"tableCode": "devmain",
					"allowFilter": true,
					"pageSizes": [
						10,
						50,
						100,
						200,
						500
					],
					"bg": "yellow"
				}
			],
			"charts": [
				{
					"name": "perf_pie",
					"type": "bar",
					"bg": "cyan",
					"filters": [
						{
							"name": "year",
							"field": ""
						},
						{
							"name": "flt2",
							"field": ""
						}
					]
				},
				{
					"name": "perf_bar",
					"title": "Deviation Performance as per ${year} in Percentage",
					"seriesTitles": [
						"Closed",
						"Raised"
					],
					"seriesData": [
						4,
						5
					],
					"type": "pie",
					"bg": "magenta"
				}
			],
			"style": {
				"grid-template-columns": "1fr 1fr",
				"grid-template-rows": "auto 250px auto 1fr",
				"grid-template-areas": "\"hdr_charts hdr_charts\" \"perf_bar  perf_pie\" \"hdr_list hdr_list\" \"dev_list dev_list\"",
				"gap": "3px"
			}
		},
		{
			"label": "Top Progress",
			"tables": [],
			"charts": [],
			"style": {
				"grid-template-columns": "",
				"grid-template-rows": "",
				"grid-template-areas": ""
			}
		},
		{
			"label": "Deviation Status",
			"tables": [],
			"charts": [],
			"style": {
				"grid-template-columns": "",
				"grid-template-rows": "",
				"grid-template-areas": ""
			}
		},
		{
			"label": "Deviation State",
			"tables": [],
			"charts": [],
			"style": {
				"grid-template-columns": "",
				"grid-template-rows": "",
				"grid-template-areas": ""
			}
		},
		{
			"label": "Pending Deviation",
			"tables": [],
			"charts": [],
			"style": {
				"grid-template-columns": "",
				"grid-template-rows": "",
				"grid-template-areas": ""
			}
		},
		{
			"label": "Print All",
			"tables": [],
			"charts": [],
			"style": {
				"grid-template-columns": "",
				"grid-template-rows": "",
				"grid-template-areas": ""
			}
		}
	]
}