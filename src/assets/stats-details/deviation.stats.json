{
	"headerHeight": "auto",
	"headerStyle": {
		"font-family": "Futura-Book",
		"padding": "5px",
		"display": "grid",
		"grid-template": "1fr / 1fr",
		"justify-items": "end",
		"align-items": "center",
		"background-color": "beige"
	},
	"filters": [
		{
			"name": "year",
			"label": "Year",
			"params": {
				"source": "devmain",
				"value": "RAISEDYEAR",
				"display": "RAISEDYEAR",
				"sort": "-RAISEDYEAR"
			},
			"data": [
				{
					"value": 2021,
					"display": "2021"
				},
				{
					"value": 2020,
					"display": "2020"
				},
				{
					"value": 2019,
					"display": "2019"
				}
			]
		}
	],
	"tabs": [
		{
			"label": "Performance",
			"nopad": true,
			"tables": [
				{
					"name": "tbl1",
					"tableCode": "an",
					"noFooter": true,
					"bg": "red",
					"filters": [
						{
							"name": "year",
							"field": ""
						},
						{
							"name": "flt2",
							"field": ""
						}
					]
				},
				{
					"name": "tbl2",
					"tableCode": "rf",
					"noFooter": true,
					"bg": "blue"
				},
				{
					"name": "tbl3",
					"tableCode": "devmain",
					"bg": "yellow"
				}
			],
			"charts": [
				{
					"name": "ch1",
					"title": "Deviation Performance as per ${year} in Percentage",
					"seriesTitles": [
						"Closed",
						"Raised"
					],
					"seriesData": [
						4,
						5
					],
					"type": "pie",
					"bg": "magenta"
				},
				{
					"name": "ch2",
					"type": "bar",
					"bg": "cyan",
					"filters": [
						{
							"name": "year",
							"field": ""
						},
						{
							"name": "flt2",
							"field": ""
						}
					]
				}
			],
			"style": {
				"grid-template-columns": "1fr repeat(2, 250px)",
				"grid-template-rows": "repeat(2, 200px) 1fr",
				"grid-template-areas": "\"ch1  tbl1  ch2\" \"ch1  tbl1  tbl2\"  \"tbl3 tbl3 tbl3\"",
				"gap": "8px"
			}
		},
		{
			"label": "Top Progress",
