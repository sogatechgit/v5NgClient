{
	"headerHeight": "0px",
	"headerStyle": {
		"font-family": "Futura-Book",
		"padding": "5px",
		"display": "grid",
		"grid-template": "1fr / 1fr",
		"justify-items": "end",
		"align-items": "center",
		"background-color": "beige"
	},
	"chartBackgrounds": [
		"#dc3545",
		"#28a745",
		"#ffc107",
		"#17a2b8",
		"#6c757d",
		"#007bff",
		"#6479a4",
		"#ffe495",
		"purple"
	],
	"chartBackgroundsBySeries": [
		{
			"series": "Raised",
			"color": "#dc3545"
		},
		{
			"series": "Closed",
			"color": "#28a745"
		}
	],
	"filters": [
		{
			"name": "year",
			"label": "Raised Year",
			"params": {
				"source": "devmain",
				"value": "RAISEDYEAR",
				"display": "RAISEDYEAR",
				"sort": "-RAISEDYEAR"
			},
			"data": []
		},
		{
			"name": "biyear",
			"label": "Raised Years",
			"params": {
				"source": "devmain",
				"value": "RAISEDYEAR",
				"minmax": true,
				"step": 1
			},
			"data": []
		},
		{
			"name": "status",
			"label": "Current Status",
			"params": {
				"source": "devmain",
				"value": "STATUS",
				"display": "STATUS",
				"sort": "STATUS"
			},
			"data": []
		},
		{
			"name": "asset",
			"label": "Asset",
			"params": {
				"source": "devmain",
				"value": "ASSET",
				"display": "ASSET",
				"sort": "ASSET"
			},
			"data": []
		}
	],
	"tabs": [
		{
			"name": "perf",
			"label": "Performance",
			"nopad": true,
			"headers": [
				{
					"name": "hdr_charts",
					"items": [
						{
							"type": "filter",
							"name": "year"
						}
					]
				},
				{
					"name": "hdr_list",
					"items": [
						{
							"type": "filter",
							"name": "asset"
						},
						{
							"type": "filter",
							"name": "status"
						}
					]
				}
			],
			"tables": [
				{
					"name": "dev_list",
					"tableCode": "devmain",
					"allowFilter": true,
					"pageSizes": [
						10,
						50,
						100,
						200,
						500
					],
					"filters": [
						{
							"name": "asset",
							"field": "ASSET"
						},
						{
							"name": "status",
							"field": "STATUS"
						}
					],
					"bg": "yellow"
				}
			],
			"charts": [
				{
					"name": "perf_pie",
					"title": "Deviation Performance in Percentage",
					"titleExpression": "Deviation Performance as per ${year} in Percentage",
					"tableCode": "devmain",
					"aggregate": "count(DEVID)",
					"series": "RSCLSTAT",
					"filters": [
						{
							"name": "year",
							"field": "RAISEDYEAR"
						}
					],
					"seriesFixedTitles": [
						"Raised",
						"Closed"
					],
					"useSeriesColors": true,
					"seriesTitles": [],
					"seriesData": [],
					"type": "pie",
					"wrapStyle": {},
					"bg": "magenta"
				},
				{
					"name": "perf_bar",
					"type": "bar",
					"wrapStyle": {},
					"filters": [
						{
							"name": "year",
							"field": ""
						}
					],
					"seriesTitles": [
						"Raised",
						"Closed"
					],
					"seriesData": [
						123,
						456
					],
					"bg": "cyan"
				}
			],
			"style": {
				"grid-template-columns": "1fr 1fr",
				"grid-template-rows": "auto 250px auto 1fr",
				"grid-template-areas": "\"hdr_charts hdr_charts\" \"perf_bar perf_pie\" \"hdr_list hdr_list\" \"dev_list dev_list\"",
				"gap": "3px"
			}
		},
		{
			"name": "topProg",
			"label": "Top Progress",
			"nopad": true,
			"headers": [
				{
					"name": "hdr_prog_charts",
					"items": [
						{
							"type": "filter",
							"name": "biyear",
							"noAll": true,
							"defLast": true
						}
					]
				},
				{
					"name": "hdr_prog_list",
					"items": [
						{
							"type": "filter",
							"name": "asset"
						},
						{
							"type": "filter",
							"name": "status"
						}
					]
				}
			],
			"tables": [
				{
					"name": "dev_prog_list",
					"tableCode": "devmain",
					"allowFilter": true,
					"pageSizes": [
						10,
						50,
						100,
						200,
						500
					],
					"filters": [
						{
							"name": "asset",
							"field": "ASSET"
						},
						{
							"name": "status",
							"field": "STATUS"
						}
					],
					"bg": "yellow"
				}
			],
			"charts": [
				{
					"name": "prog_bar",
					"type": "bar",
					"wrapStyle": {},
					"filters": [
						{
							"name": "year",
							"field": ""
						}
					],
					"bg": "cyan"
				},
				{
					"name": "prog_pie1",
					"title": "Deviation Performance as per ${biyear} in Percentage",
					"tableCode": "devmain",
					"aggregate": "count(DEVID)",
					"series": "RSCLSTAT",
					"seriesFixedTitles": [
						"Raised",
						"Closed"
					],
					"seriesTitles": [],
					"seriesData": [],
					"type": "pie",
					"wrapStyle": {},
					"filters": [
						{
							"name": "biyear:0",
							"field": "RAISEDYEAR"
						}
					],
					"bg": "magenta"
				},
				{
					"name": "prog_pie2",
					"title": "Deviation Performance as per ${biyear} in Percentage",
					"tableCode": "devmain",
					"aggregate": "count(DEVID)",
					"series": "RSCLSTAT",
					"seriesFixedTitles": [
						"Raised",
						"Closed"
					],
					"seriesTitles": [],
					"seriesData": [],
					"type": "pie",
					"wrapStyle": {},
					"filters": [
						{
							"name": "biyear:1",
							"field": "RAISEDYEAR"
						}
					],
					"bg": "magenta"
				}
			],
			"style": {
				"grid-template-columns": "1fr 1fr",
				"grid-template-rows": "auto 150px 150px auto 1fr",
				"grid-template-areas": "\"hdr_prog_charts hdr_prog_charts\" \"prog_bar prog_pie1\" \"prog_bar prog_pie2\" \"hdr_prog_list hdr_prog_list\" \"dev_prog_list dev_prog_list\""
			}
		},
		{
			"name": "devStatus",
			"label": "Deviation Status",
			"tables": [],
			"charts": [],
			"style": {
				"grid-template-columns": "",
				"grid-template-rows": "",
				"grid-template-areas": ""
			}
		},
		{
			"name": "devState",
			"label": "Deviation State",
			"tables": [],
			"charts": [],
			"style": {
				"grid-template-columns": "",
				"grid-template-rows": "",
				"grid-template-areas": ""
			}
		},
		{
			"name": "devPending",
			"label": "Pending Deviation",
			"tables": [],
			"charts": [],
			"style": {
				"grid-template-columns": "",
				"grid-template-rows": "",
				"grid-template-areas": ""
			}
		},
		{
			"name": "print",
			"label": "Print All",
			"tables": [],
			"charts": [],
			"style": {
				"grid-template-columns": "",
				"grid-template-rows": "",
				"grid-template-areas": ""
			}
		}
	]
}